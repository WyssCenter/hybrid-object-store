  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src",
      "<rootDir>/.storybook"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.stories.tsx"
    ],
    "setupFiles": [
      "<rootDir>/src/setupJest.js",
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js",
      "<rootDir>/__mocks__/LocalStorage.js"
    ],
    "testMatch": [
      "<rootDir>/.storybook/storybook.test.js",
      "<rootDir>/src/**/__tests__/**/*.test.{js,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "<rootDir>/node_modules/jest-circus/runner.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js",
      "^.+\\.svg$": "<rootDir>/config/jest/svgTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^Styles[/](.+)": "<rootDir>/src/styles/$1",
      "^Hooks[/](.+)": "<rootDir>/src/hooks/$1",
      "^Shared[/](.+)": "<rootDir>/src/shared/$1",
      "^Fonts[/](.+)": "<rootDir>/src/fonts/$1",
      "^Images[/](.+)": "<rootDir>/src/images/$1",
      "^Components[/](.+)": "<rootDir>/src/components/$1",
      "^Layout[/](.+)": "<rootDir>/src/layout/$1",
      "^Environment[/](.+)": "<rootDir>/src/environment/$1",
      "^Pages[/](.+)": "<rootDir>/src/pages/$1",
      "^Src[/](.+)": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "automock": false,
    "resetMocks": false
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "require-context.macro": "^1.2.2",
    "ts-jest": "^26.5.5"
  }
